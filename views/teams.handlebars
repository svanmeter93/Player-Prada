{{#each teams as |team|}}

<div id={{team.name}}>
    <p>Name: {{team.name}}</p>
    <p>Wins: {{team.wins}}</p>
    <p>Loses: {{team.losses}}</p>
    <button>Favorite team?</button>
</div>
{{/each}}
